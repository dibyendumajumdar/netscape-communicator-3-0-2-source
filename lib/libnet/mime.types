exts="qt,mov,moov"              type=video/quicktime            \
desc="Quicktime Video"          icon=internal-gopher-movie

exts="avi"                      type=video/x-msvideo            \
desc="Microsoft Video"          icon=internal-gopher-movie


####################################################################
