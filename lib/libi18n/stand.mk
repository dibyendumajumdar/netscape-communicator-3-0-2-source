all:	s2j s2e j2s j2e e2j e2s ads2j: codesets.h sjis2jis.c	cc -DSTANDALONE_TEST sjis2jis.c -o s2js2e: codesets.h sjis2euc.c	cc -DSTANDALONE_TEST sjis2euc.c -o s2ej2s: codesets.h jis2oth.c	cc -DSTANDALONE_TEST -DEUC_TEST=0 jis2oth.c -o j2sj2e: codesets.h jis2oth.c	cc -DSTANDALONE_TEST -DEUC_TEST=1 jis2oth.c -o j2ee2j: codesets.h euc2jis.c	cc -DSTANDALONE_TEST euc2jis.c -o e2je2s: codesets.h euc2sjis.c	cc -DSTANDALONE_TEST euc2sjis.c -o e2sad: codesets.h autodetect.c	cc -g -DSTANDALONE_TEST autodetect.c -o adclean:	rm s2j s2e j2s j2e e2j e2s